@page
@model SampleRazorApp.Pages.KukuModel
@{
    ViewData["Title"] = "kuku";
}

<h1>kuku</h1>

<table class="table table-bordered table-sm table-hover">
    @for (int i = 1; i <= 9; i++)
    {
    <tr>
        @for(int j = 1; j<= 9; j++){
        <td class="text-right">
            @(i*j)
        </td>
        }
    </tr>
    }
</table>